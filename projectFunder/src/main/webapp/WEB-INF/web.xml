<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Babble</display-name>
  <servlet>
    <servlet-name>freemarker</servlet-name>
    <servlet-class>freemarker.ext.servlet.FreemarkerServlet</servlet-class>
    <init-param>
      <param-name>TemplatePath</param-name>
      <param-value>WEB-INF</param-value>
    </init-param>
    <init-param>
      <param-name>NoCache</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>ContentType</param-name>
      <param-value>text/html; charset=UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>template_update_delay</param-name>
      <param-value>0</param-value>
    </init-param>
    <init-param>
      <param-name>default_encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>number_format</param-name>
      <param-value>0.##########</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>freemarker</servlet-name>
    <url-pattern>*.ftl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>hello_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hello_servlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>projectFunder_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.ProjectFunderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>projectFunder_servlet</servlet-name>
    <url-pattern>/projectFunder</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newProject_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.NewProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newProject_servlet</servlet-name>
    <url-pattern>/NewProjectServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewProject_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.ViewProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewProject_servlet</servlet-name>
    <url-pattern>/ViewProjectServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>viewProfile_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.ViewProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewProfile_servlet</servlet-name>
    <url-pattern>/ViewProfileServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editProject_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.EditProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editProject_servlet</servlet-name>
    <url-pattern>/EditProjectServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newComment_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.NewCommentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newComment_servlet</servlet-name>
    <url-pattern>/NewCommentServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>newFund_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.NewFundServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newFund_servlet</servlet-name>
    <url-pattern>/NewFundServlet</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>searchProject_servlet</servlet-name>
    <servlet-class>de.unidue.inf.is.SearchProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchProject_servlet</servlet-name>
    <url-pattern>/SearchProjectServlet</url-pattern>
  </servlet-mapping>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>FreeMarker MVC Views</web-resource-name>
      <url-pattern>*.ftl</url-pattern>
    </web-resource-collection>
    <auth-constraint/>
  </security-constraint>
</web-app>